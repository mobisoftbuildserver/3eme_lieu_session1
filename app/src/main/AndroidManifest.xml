<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.orange.thirdplace.applicationtest">

    <uses-permission android:name="android.permission.TRANSMIT_IR"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.orange.thirdplace.applicationtest.ActivityEnd"
            android:label="@string/title_activity_end"
           android:parentActivityName="com.orange.thirdplace.applicationtest.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.orange.thirdplace.applicationtest.MainActivity" />
        </activity>

    </application>

</manifest>